	"main": "dist/server.js",
	"scripts": {
		"start": "node -r ./tsconfig-paths-bootstrap.js dist/server.js",
		"dev": "nodemon --watch src --exec ts-node -r tsconfig-paths/register src/server.ts",
		"build": "tsc",
		"lint": "eslint . --ext .ts",
		"test": "npx jest --watch"
	},
	"dependencies": {
		"compression": "1.8.1",
