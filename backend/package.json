		"hpp": "0.2.3",
		"morgan": "1.10.1",
		"tsconfig-paths": "4.2.0",
		"zod": "4.2.1"
	},
	"devDependencies": {
		"@types/compression": "1.8.1",
		"@types/cors": "2.8.19",
		"@types/express": "5.0.6",
		"@types/hpp": "0.2.7",
		"@types/jest": "30.0.0",
		"@types/morgan": "1.9.10",
		"@types/node": "25.0.3",
		"@types/supertest": "6.0.3",
		"@typescript-eslint/eslint-plugin": "8.50.1",
		"@typescript-eslint/parser": "8.50.1",
		"eslint": "9.39.2",
		"eslint-plugin-no-relative-import-paths": "1.6.1",
		"jest": "30.2.0",
		"nodemon": "3.1.11",
		"supertest": "7.1.4",
		"ts-jest": "29.4.6",
		"ts-node": "10.9.2",
		"typescript": "5.9.3"
